{
  "name": "chatlive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:1-generate": "prisma generate",
    "build:2-migrate": "prisma migrate deploy",
    "build:3-next": "cross-env NODE_ENV=production next build",
    "build:4-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:next": "next dev",
    "dev:wss": "cross-env PORT=3001 nodemon --watch src --ext .ts,.tsx,js,jsx --signal SIGTERM --exec \"ts-node --project tsconfig.server.json src/server/wssDevServer.ts\"",
    "dev": "run-p dev:*",
    "start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
    "lint": "next lint",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.1.1",
    "@trpc/client": "^9.26.2",
    "@trpc/next": "^9.26.2",
    "@trpc/react": "^9.26.2",
    "@trpc/server": "^9.26.2",
    "nanoid": "^4.0.0",
    "next": "12.2.3",
    "next-auth": "^4.10.3",
    "node-fetch": "2.6.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.2",
    "superjson": "^1.9.1",
    "ws": "^8.8.1",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/node": "18.0.0",
    "@types/node-fetch": "2.5.11",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/ws": "^8.5.3",
    "autoprefixer": "^10.4.8",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "8.18.0",
    "eslint-config-next": "12.2.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prisma": "^4.1.1",
    "tailwindcss": "^3.1.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  }
}
